    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "faker": "^5.5.3",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "react-icons": "^4.3.1",
    "react-scripts": "^3.4.4",
    "sequelize": "^6.9.0"
  },
