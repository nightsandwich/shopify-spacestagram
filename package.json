    "@babel/preset-react": "^7.16.0",
    "@babel/register": "^7.16.0",
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.1.0",
    "@mui/material": "^5.1.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
    "axios-mock-adapter": "^1.20.0",
    "babel-loader": "^8.2.3",
